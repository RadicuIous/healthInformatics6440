<?xml version="1.0" encoding="UTF-8"?><project>
  <actions/>
  <description/>
  <logRotator>
    <daysToKeep>20</daysToKeep>
    <numToKeep>-1</numToKeep>
    <artifactDaysToKeep>-1</artifactDaysToKeep>
    <artifactNumToKeep>-1</artifactNumToKeep>
  </logRotator>
  <keepDependencies>false</keepDependencies>
  <properties/>
  <scm class="hudson.scm.NullSCM"/>
  <triggers class="vector">
  </triggers>
  <assignedNode>master</assignedNode>
  <canRoam>false</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <concurrentBuild>false</concurrentBuild>
  <customWorkspace>$JENKINS_HOME/workspace/$BUILD_TAG</customWorkspace>
  <builders>
    <hudson.tasks.Shell>
      <command>env</command>
    </hudson.tasks.Shell>

    <hudson.plugins.copyartifact.CopyArtifact>
      <projectName>$TRIGGERING_PROJECT</projectName>
      <filter>pipeline.properties</filter>
      <target/>
      <selector class="hudson.plugins.copyartifact.StatusBuildSelector"/>
    </hudson.plugins.copyartifact.CopyArtifact>

    <org.jenkinsci.plugins.envinject.EnvInjectBuilder>
      <info>
        <propertiesFilePath>pipeline.properties</propertiesFilePath>
      </info>
    </org.jenkinsci.plugins.envinject.EnvInjectBuilder>

    <hudson.plugins.copyartifact.CopyArtifact>
      <projectName>$TRIGGERING_PROJECT</projectName>
      <filter>$TRIGGERING_PROJECT_WORKSPACE_ARTIFACT</filter>
      <target/>
      <selector class="hudson.plugins.copyartifact.StatusBuildSelector"/>
    </hudson.plugins.copyartifact.CopyArtifact>

    <hudson.tasks.Shell>
      <command>tar -xzf $TRIGGERING_PROJECT_WORKSPACE_ARTIFACT &amp;&amp; rm $TRIGGERING_PROJECT_WORKSPACE_ARTIFACT
      </command>
    </hudson.tasks.Shell>

    <hudson.tasks.Shell>
      <command>./scripts/publish-wars $PROMOTED_REVISION</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
    <hudson.plugins.statusmonitor.MonitorPublisher/>
    <hudson.plugins.ws__cleanup.WsCleanup plugin="ws-cleanup@0.10">
      <deleteDirs>false</deleteDirs>
      <skipWhenFailed>false</skipWhenFailed>
    </hudson.plugins.ws__cleanup.WsCleanup>
  </publishers>
  <buildWrappers>
    <hudson.plugins.build__timeout.BuildTimeoutWrapper>
      <timeoutMinutes>3</timeoutMinutes>
      <failBuild>false</failBuild>
      <writingDescription>false</writingDescription>
      <timeoutPercentage>0</timeoutPercentage>
      <timeoutType>likelyStuck</timeoutType>
      <timeoutMinutesElasticDefault>3</timeoutMinutesElasticDefault>
    </hudson.plugins.build__timeout.BuildTimeoutWrapper>
    <hudson.plugins.ansicolor.AnsiColorBuildWrapper/>
  </buildWrappers>
</project>