<?xml version="1.0" encoding="UTF-8"?><project>
  <actions/>
  <description/>
  <logRotator>
    <daysToKeep>30</daysToKeep>
    <numToKeep>-1</numToKeep>
    <artifactDaysToKeep>-1</artifactDaysToKeep>
    <artifactNumToKeep>5</artifactNumToKeep>
  </logRotator>
  <keepDependencies>false</keepDependencies>
  <properties/>
  <scm class="hudson.scm.NullSCM"/>
  <assignedNode>BrowserTest</assignedNode>
  <canRoam>false</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers class="vector"/>
  <concurrentBuild>false</concurrentBuild>
  <customWorkspace>$JENKINS_HOME/workspace/$BUILD_TAG</customWorkspace>
  <builders>
    <hudson.plugins.copyartifact.CopyArtifact>
      <projectName>$TRIGGERING_PROJECT</projectName>
      <filter>pipeline.properties</filter>
      <target/>
      <selector class="hudson.plugins.copyartifact.StatusBuildSelector"/>
    </hudson.plugins.copyartifact.CopyArtifact>
    <org.jenkinsci.plugins.envinject.EnvInjectBuilder>
      <info>
        <propertiesFilePath>pipeline.properties</propertiesFilePath>
      </info>
    </org.jenkinsci.plugins.envinject.EnvInjectBuilder>
    <hudson.plugins.copyartifact.CopyArtifact>
      <projectName>$TRIGGERING_PROJECT</projectName>
      <filter>functional_tests_$PROMOTED_REVISION.zip</filter>
      <target/>
      <selector class="hudson.plugins.copyartifact.StatusBuildSelector"/>
    </hudson.plugins.copyartifact.CopyArtifact>
    <hudson.tasks.BatchFile>
      <command>rd /S /Q functional_tests
mkdir functional_tests
move functional_tests_%PROMOTED_REVISION%.zip functional_tests/
cd functional_tests
jar xf functional_tests_%PROMOTED_REVISION%.zip
ui-component-test.bat
      </command>
    </hudson.tasks.BatchFile>
  </builders>
  <publishers>
    <hudson.tasks.ArtifactArchiver>
      <artifacts>functional_tests\ui-component-tests\src\main\cucumber\ui_component_tests_report.html,functional_tests\target\ui-component-tests\screenshots\**\*,functional_tests\target\ui-component-tests\webdriver.zip</artifacts>
      <latestOnly>false</latestOnly>
    </hudson.tasks.ArtifactArchiver>
    <org.jvnet.hudson.plugins.groovypostbuild.GroovyPostbuildRecorder>
      <groovyScript>import hudson.model.*

def envVarsMap = manager.build.properties.get("envVars")

manager.listener.getLogger().println("JOBNAME" + ": " + envVarsMap.get("JOB_NAME"))
manager.listener.getLogger().println("BUILD_NUMBER" + ": " + envVarsMap.get("BUILD_NUMBER"))
manager.listener.getLogger().println("PROMOTED_REVISION" + ": " + envVarsMap.get("PROMOTED_REVISION"))


List&lt;hudson.model.StringParameterValue&gt; values2 = new ArrayList&lt;StringParameterValue&gt;();
values2.add(new StringParameterValue("JOBNAME", envVarsMap.get("JOB_NAME") ));
values2.add(new StringParameterValue("BUILDNO", envVarsMap.get("BUILD_NUMBER") ));
values2.add(new StringParameterValue("PROMOTED_REVISION", envVarsMap.get("PROMOTED_REVISION")
));
ParametersAction actionWithParams = new ParametersAction(values2);

def jobRun = manager.hudson.getItem("SCMS_Archive_Tests")

manager.hudson.queue.schedule(jobRun, 0, actionWithParams);
      </groovyScript>
      <behavior>0</behavior>
    </org.jvnet.hudson.plugins.groovypostbuild.GroovyPostbuildRecorder>
    <hudson.plugins.statusmonitor.MonitorPublisher/>
    <hudson.plugins.ws__cleanup.WsCleanup plugin="ws-cleanup@0.10">
      <deleteDirs>false</deleteDirs>
      <skipWhenFailed>false</skipWhenFailed>
    </hudson.plugins.ws__cleanup.WsCleanup>
  </publishers>
  <buildWrappers>
    <hudson.plugins.build__timeout.BuildTimeoutWrapper>
      <timeoutMinutes>3</timeoutMinutes>
      <failBuild>false</failBuild>
      <writingDescription>false</writingDescription>
      <timeoutPercentage>0</timeoutPercentage>
      <timeoutType>likelyStuck</timeoutType>
      <timeoutMinutesElasticDefault>3</timeoutMinutesElasticDefault>
    </hudson.plugins.build__timeout.BuildTimeoutWrapper>
    <hudson.plugins.ansicolor.AnsiColorBuildWrapper/>
    <hudson.plugins.locksandlatches.LockWrapper>
      <locks>
        <hudson.plugins.locksandlatches.LockWrapper_-LockWaitConfig>
          <name>BrowserTest</name>
        </hudson.plugins.locksandlatches.LockWrapper_-LockWaitConfig>
      </locks>
    </hudson.plugins.locksandlatches.LockWrapper>
  </buildWrappers>
</project>