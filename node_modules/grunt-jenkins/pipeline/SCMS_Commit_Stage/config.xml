<?xml version="1.0" encoding="UTF-8"?><project>
  <actions/>
  <description/>
  <logRotator>
    <daysToKeep>20</daysToKeep>
    <numToKeep>-1</numToKeep>
    <artifactDaysToKeep>-1</artifactDaysToKeep>
    <artifactNumToKeep>5</artifactNumToKeep>
  </logRotator>
  <keepDependencies>false</keepDependencies>
  <properties/>
  <scm class="hudson.scm.NullSCM"/>
  <assignedNode>master</assignedNode>
  <canRoam>false</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers class="vector"/>
  <concurrentBuild>false</concurrentBuild>
  <customWorkspace>$JENKINS_HOME/workspace/$BUILD_TAG</customWorkspace>
  <builders>
    <hudson.tasks.Shell>
      <command>env</command>
    </hudson.tasks.Shell>
    <hudson.plugins.copyartifact.CopyArtifact>
      <projectName>$TRIGGERING_PROJECT</projectName>
      <filter>pipeline.properties</filter>
      <target/>
      <selector class="hudson.plugins.copyartifact.StatusBuildSelector"/>
    </hudson.plugins.copyartifact.CopyArtifact>
    <org.jenkinsci.plugins.envinject.EnvInjectBuilder>
      <info>
        <propertiesFilePath>pipeline.properties</propertiesFilePath>
      </info>
    </org.jenkinsci.plugins.envinject.EnvInjectBuilder>
    <hudson.plugins.copyartifact.CopyArtifact>
      <projectName>$TRIGGERING_PROJECT</projectName>
      <filter>$TRIGGERING_PROJECT_WORKSPACE_ARTIFACT</filter>
      <target/>
      <selector class="hudson.plugins.copyartifact.StatusBuildSelector"/>
    </hudson.plugins.copyartifact.CopyArtifact>
    <hudson.tasks.Shell>
      <command>tar -xmzf $TRIGGERING_PROJECT_WORKSPACE_ARTIFACT &amp;&amp; rm $TRIGGERING_PROJECT_WORKSPACE_ARTIFACT</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>cd deploy/scripts
        cat sonar-verify.sh
        echo ./sonar-verify.sh
      </command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>./gradlew service-component-tests:features -PcollectCoverage=true --info -PbuildEnvironment=ci</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>tar -czf $BUILD_TAG.tgz *</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
    <hudson.tasks.ArtifactArchiver>
      <artifacts>pipeline.properties,service-component-tests/src/main/cucumber/service_component_tests_report.html,$BUILD_TAG.tgz,service/target/artifacts/*.war,functional_tests_$PROMOTED_REVISION.zip,target/deploy/puppet.tar.gz</artifacts>
      <latestOnly>false</latestOnly>
    </hudson.tasks.ArtifactArchiver>
    <hudson.plugins.postbuildtask.PostbuildTask>
      <tasks>
        <hudson.plugins.postbuildtask.TaskProperties>
          <logTexts>
            <hudson.plugins.postbuildtask.LogProperties>
              <logText/>
              <operator>AND</operator>
            </hudson.plugins.postbuildtask.LogProperties>
          </logTexts>
          <EscalateStatus>false</EscalateStatus>
          <RunIfJobSuccessful>false</RunIfJobSuccessful>
          <script>/cidata/build-tools/scripts/archive-artifacts.sh /opt/base/jenkins/jobs/$JOB_NAME/builds /cidata/scms $PROMOTED_REVISION $BUILD_NUMBER $JOB_NAME
          </script>
        </hudson.plugins.postbuildtask.TaskProperties>
      </tasks>
    </hudson.plugins.postbuildtask.PostbuildTask>
    <hudson.plugins.statusmonitor.MonitorPublisher/>
    <hudson.plugins.parameterizedtrigger.BuildTrigger>
      <configs>
        <hudson.plugins.parameterizedtrigger.BuildTriggerConfig>
          <configs>
            <hudson.plugins.parameterizedtrigger.PredefinedBuildParameters>
              <properties>TRIGGERING_PROJECT=$JOB_NAME
                TRIGGERING_PROJECT_WORKSPACE_ARTIFACT=$BUILD_TAG.tgz</properties>
            </hudson.plugins.parameterizedtrigger.PredefinedBuildParameters>
          </configs>
          <projects>SCMS_CI_Deploy,SCMS_Metrics,SCMS_Nagios</projects>
          <condition>SUCCESS</condition>
          <triggerWithNoParameters>false</triggerWithNoParameters>
        </hudson.plugins.parameterizedtrigger.BuildTriggerConfig>
      </configs>
    </hudson.plugins.parameterizedtrigger.BuildTrigger>
    <hudson.plugins.ws__cleanup.WsCleanup plugin="ws-cleanup@0.10">
      <deleteDirs>false</deleteDirs>
      <skipWhenFailed>false</skipWhenFailed>
    </hudson.plugins.ws__cleanup.WsCleanup>
  </publishers>
  <buildWrappers>
    <hudson.plugins.build__timeout.BuildTimeoutWrapper>
      <timeoutMinutes>3</timeoutMinutes>
      <failBuild>false</failBuild>
      <writingDescription>false</writingDescription>
      <timeoutPercentage>0</timeoutPercentage>
      <timeoutType>likelyStuck</timeoutType>
      <timeoutMinutesElasticDefault>3</timeoutMinutesElasticDefault>
    </hudson.plugins.build__timeout.BuildTimeoutWrapper>
    <hudson.plugins.ansicolor.AnsiColorBuildWrapper/>
    <hudson.plugins.locksandlatches.LockWrapper>
      <locks>
        <hudson.plugins.locksandlatches.LockWrapper_-LockWaitConfig>
          <name>SCMS Commit Stage</name>
        </hudson.plugins.locksandlatches.LockWrapper_-LockWaitConfig>
      </locks>
    </hudson.plugins.locksandlatches.LockWrapper>
  </buildWrappers>
</project>